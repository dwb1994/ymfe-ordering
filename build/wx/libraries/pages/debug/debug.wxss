.env-info-container {
    margin: 10px;
    text-align: center;
}

.env-info {
    background: rgb(248, 248, 248);
    width: 100%;
    border-radius: 5px;
    box-sizing: border-box;
    padding: 5px;
}

.operate-first-btn {
    margin: 10px;
}

.operate-btn {
    margin: 30px 10px 10px;
}

.edit-env-container {
    margin: 5px;
    text-align: center;
    background:rgb(248, 248, 248);
    border-radius:5px;
    padding-bottom:10px;
}
.radio-group-env {
    margin-top: 10px;
}
.env-name {
    margin-bottom: 5px;
}
.env-row {
    margin-top: 15px;
}
.env-row.app {
    border-bottom:1px dashed;
    padding-bottom:8px;
}
.env-input {
    border: 1px solid rgba(0, 0, 0, 0.2);
    border-radius: 3px;
    margin-left: 8px;
    vertical-align: middle;
    padding-left:5px;
    background:#fff;
}
.header {
    margin-top: 20px;
    display: flex;
}
.header-btn {
    margin: 0 5px 0 5px;
    justify-content: space-between;
    flex-grow: 1;
}
